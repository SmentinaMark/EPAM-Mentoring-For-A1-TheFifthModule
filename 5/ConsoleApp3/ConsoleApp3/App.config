<?xml version="1.0" encoding="utf-8" ?>
<configuration>

	<configSections>
		<section name="CustomSection" type="ConsoleApp3.StartupCustomSection, ConsoleApp3"/>
	</configSections>
	
	<CustomSection Name="ConfigSection">
		<CustomElement>
			<add key="Language" value="en"/>
      
			<add key="Number" boolValue="true"/>

      <add key="Date" boolValue="false"/>

      <add key="FoldersC" value= "C:\"/>
      <add key="FoldersGames" value= "C:\Games"/>
      
		</CustomElement>
	</CustomSection>

	<appSettings>
		<add key=".txt" value="Текстовые файлы" />
		<add key=".jpg" value="Картинки" />
		<add key=".docx" value="Документы" />
	</appSettings>
</configuration>
